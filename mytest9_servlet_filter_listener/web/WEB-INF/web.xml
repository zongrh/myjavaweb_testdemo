    <!-- Servlet配置 -->
    <servlet>
        <servlet-name>MessageServlet</servlet-name>
        <servlet-class>mypackage6.MessageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MessageServlet</servlet-name>
        <url-pattern>/MessageServlet</url-pattern>
    </servlet-mapping>
    <!-- 过滤器配置 -->
    <filter>
        <filter-name>WordFilter</filter-name>
        <filter-class>mypackage6.WordFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>GBK</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>WordFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!-- 主页面 -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    <listener>
        <listener-class>mypackage7.Listenerlocalhost</listener-class>
    </listener>
